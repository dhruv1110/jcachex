<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">

    <!--
        OWASP Dependency Check Suppression Configuration for JCacheX

        This file contains suppressions for known false positives and accepted risks.
        Each suppression should include:
        1. Clear justification
        2. CVE details
        3. Review date for periodic reassessment
    -->

    <!-- Example suppressions - update based on actual scan results -->

    <!-- Suppress test-only dependencies -->
    <suppress>
        <notes>
            JUnit vulnerabilities don't affect production code as it's test-only.
            Last reviewed: 2024-01-01
        </notes>
        <packageUrl regex="true">^pkg:maven/org\.junit\..*@.*$</packageUrl>
        <cve>CVE-2020-15250</cve>
    </suppress>

    <!-- Suppress Kotlin standard library false positives -->
    <suppress>
        <notes>
            Kotlin stdlib false positive - not a security vulnerability in our context.
            Last reviewed: 2024-01-01
        </notes>
        <packageUrl regex="true">^pkg:maven/org\.jetbrains\.kotlin/kotlin-stdlib.*@.*$</packageUrl>
        <vulnerabilityName>CVE-2022-24329</vulnerabilityName>
    </suppress>

    <!-- Suppress development tool vulnerabilities -->
    <suppress>
        <notes>
            Gradle and build tool vulnerabilities don't affect runtime.
            Last reviewed: 2024-01-01
        </notes>
        <packageUrl regex="true">^pkg:maven/org\.gradle.*@.*$</packageUrl>
    </suppress>

    <!--
        Template for new suppressions:

        <suppress>
            <notes>
                Justification: [Why this is safe to suppress]
                Impact: [Assessment of actual impact]
                Last reviewed: [Date]
            </notes>
            <packageUrl regex="true">^pkg:maven/group\.id/artifact-id@.*$</packageUrl>
            <cve>CVE-YYYY-NNNNN</cve>
        </suppress>
    -->

</suppressions>
